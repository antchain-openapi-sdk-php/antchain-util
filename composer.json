{
  "name": "antchain/alipay-util",
  "description": "Ant Chain Util Library for PHP",
  "type": "library",
  "license": "Apache-2.0",
  "authors": [],
  "require": {
    "php": ">5.5",
    "alibabacloud/tea-utils": "^0.2.3",
    "phpunit/phpunit": "~4.8.35"
  },
  "autoload": {
    "psr-4": {
      "AntChain\\Alipay\\Util\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AntChain\\Alipay\\Util\\Tests\\": "tests"
    }
  },
  "scripts": {
    "fixer": "php-cs-fixer fix ./",
    "test": [
      "./vendor/bin/phpunit --colors=always"
    ]
  },
  "config": {
    "sort-packages": true,
    "preferred-install": "dist",
    "optimize-autoloader": true
  },
  "prefer-stable": true
}